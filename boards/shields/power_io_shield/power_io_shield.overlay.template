#include <ardep/dt-bindings/power-io-shield.h>

&i2c2 {
	status = "okay";

	power_io_shield%id%: power_io_shield%id%@2%id% {
		compatible = "power-io-shield";
		int-gpios = <&gpioe 2 (GPIO_ACTIVE_LOW|GPIO_PULL_UP)>, <&gpioe 5 (GPIO_ACTIVE_LOW|GPIO_PULL_UP)>;
		reg = <0x2%id%>;
		gpio-controller;
		#gpio-cells = <2>;
	};
};
